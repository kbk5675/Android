                        style="@style/Widget.View.ValueTextView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/label_color"
                        />

                </LinearLayout>
                <View android:layout_width="match_parent" android:layout_height="1dp" android:background="@color/split_line_color" />

                <!-- Power Gain -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:padding="3dp" >

                    <TextView
                        style="@style/Widget.View.LabelTextView"
                        android:layout_width="350dp"
                        android:layout_height="23dp"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:text="@string/write_power" />

                    <com.xw.repo.BubbleSeekBar
                        android:id="@+id/seekbar_power"
                        android:layout_width="208dp"
                        android:layout_height="23dp"
                        app:bsb_bubble_color="@color/G778198"
                        app:bsb_bubble_text_color="@color/white"
                        app:bsb_second_track_color="@color/Rc4e0fd"
                        app:bsb_section_text_color="@color/B3A3A3A"
                        app:bsb_section_text_position="bottom_sides"
                        app:bsb_thumb_color="@color/R095a6f"
                        app:bsb_thumb_radius="0dp"
                        app:bsb_thumb_radius_on_dragging="0dp"
                        app:bsb_track_color="@color/GD1D6DB"
                        app:bsb_track_size="8dp" />

                    <View
                        android:layout_width="3dp"
                        android:layout_height="3dp" />

                </LinearLayout>


                <!-- Bank -->
                <!-- Write Data -->

            </LinearLayout>

        </ScrollView>

        <View android:layout_width="match_parent" android:layout_height="5dp" />

        <!-- Buttons -->
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TableRow >

                <Button android:id="@+id/actionRead"
                    style="@style/Widget.AppCompat.Button.Colored"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/action_read"
                    />

                <Button android:id="@+id/clear"
                    style="@style/Widget.AppCompat.Button.Colored"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/action_clear" />

            </TableRow>

            <TableRow >

                <Button
                    android:id="@+id/actionhex"
                    style="@style/Widget.AppCompat.Button.Colored"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/write" />

            </TableRow>

        </TableLayout>

    </LinearLayout>

</LinearLayout>
